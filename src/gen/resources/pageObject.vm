import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.CacheLookup;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

import selenium.com.AbstractSeleniumPage;


/**
 * ${pageBean.classNm}クラス<br>
 */
public class ${pageBean.classNm} extends AbstractSeleniumPage {

#foreach(${textBean} in ${pageBean.typeTextList})
    @FindBy(${textBean.findBy} = ${q}${textBean.findByValue}${q})
    @CacheLookup
    /** ${textBean.item} text */
    private WebElement ${textBean.item};

#end

    /**
     * PageFactoryを使用してWebElementをマッピングする
     * @return ${pageBean.classNm}
     */
    public ${pageBean.classNm} initialize() {
        return PageFactory.initElements(driver, this.getClass());
    }

#foreach(${textBean} in ${pageBean.typeTextList})
    /**
     * ${textBean.item} textに値を設定します。
     * @param text
     */
    public void set${textBean.itemUpper}(String text) {
        this.${textBean.item}.sendKeys(text);
    }

#end

}
